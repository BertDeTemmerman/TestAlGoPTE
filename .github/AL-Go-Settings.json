{
  "type": "PTE",
  "templateUrl": "https://github.com/microsoft/AL-Go-PTE@main",
  "githubrunner": "self-hosted",
  "environments": ["QA"],
  "CICDPushBranches": [ "main", "release/*", "feature/*", "UAT", "development" ]
  "CICDPullRequestBranches": [ "main", "release/*", "feature/*", "UAT", "development" ]
  "DeployToQA":  {
        "Branches": [ "UAT", "main" ],
        "EnvironmentName": "QA",
        "Runs-on": "Windows-2019",
        "SyncMode": "ForceSync"
  },
  "buildModes": [ "Translated" ],
  "enableCodeCop": "true",
  "enableUICop": "true",
  "failOn": "warning"
}
