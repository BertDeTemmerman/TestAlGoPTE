{
  "type": "PTE",
  "templateUrl": "https://github.com/microsoft/AL-Go-PTE@main",
  "CICDPushBranches": [
    "main",
    "release/*",
    "feature/*",
    "UAT"
  ],
  "CICDPullRequestBranches": [
    "main",
    "release/*",
    "feature/*",
    "UAT",
    "development"
  ],
  "ConditionalSettings": [
    {
      "branches": [
        "development"
      ],
      "settings": {
        "CICDSchedule": "5 * * * *"
      }
    }
  ],  
  "enableCodeCop": true,
  "enableUICop": true
}

