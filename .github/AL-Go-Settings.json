{
  "type": "PTE",
  "templateUrl": "https://github.com/microsoft/AL-Go-PTE@main",
  "githubrunner": "self-hosted",
  "CICDPushBranches": [
    "main",
    "release/*",
    "feature/*",
    "UAT"
  ],
  "CICDPullRequestBranches": [
    "main",
    "release/*",
    "feature/*",
    "UAT",
    "development"
  ],
  "enableCodeCop": true,
  "enableUICop": true
}
